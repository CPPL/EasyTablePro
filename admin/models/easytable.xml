<root>
	<params group="EASYTABLE_PREFS">
		<param name="access" type="list" size="20" default="0" label="Access Level" description="COM_EASYTABLEPRO_PREF_ACCESS_LEVEL_DESC">
			<option value="0">Public</option>
			<option value="1">Registered</option>
			<option value="2">Special</option>
		</param>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="show_description" type="list" default="" label="COM_EASYTABLEPRO_PREF_SHOW_DESCRIPTION" description="COM_EASYTABLEPRO_PREF_SHOW_DESCRIPTION_DESC">
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="show_search" type="list" default="" label="COM_EASYTABLEPRO_PREF_SHOW_TABLE_SEARCH" description="COM_EASYTABLEPRO_PREF_SHOW_TABLE_SEARCH_DESC">
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="show_pagination" type="list" default="" label="COM_EASYTABLEPRO_PREF_ENABLE_TABLE_PAGINATION" description="COM_EASYTABLEPRO_PREF_ENABLE_TABLE_PAGINATION">
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="show_pagination_header" type="list" default="" label="COM_EASYTABLEPRO_PREF_SHOW_PAGES_DESC" description="COM_EASYTABLEPRO_PREF_PAGINATION_CONTROLS_DESC">
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="show_pagination_footer" type="list" default="" label="COM_EASYTABLEPRO_PREF_SHOW_TABLE_PAGINATION_FOOTER" description="COM_EASYTABLEPRO_PREF_SHOW_TABLE_PAGINATION_FOOTER_DESC">
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="show_created_date" label="COM_EASYTABLEPRO_PREF_SHOW_CREATION_DATE" type="list" default="" description="COM_EASYTABLEPRO_PREF_CREATION_DATE_DISP_DESC">
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="show_modified_date" label="COM_EASYTABLEPRO_PREF_SHOW_MODIFIED_DATE" type="list" default="" description="COM_EASYTABLEPRO_PREF_SHOW_MODIFIED_DATE_DESC">
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="modification_date_label" label="COM_EASYTABLEPRO_PREF_MODIFICATION_DATE_LABEL" type="text" default="" description="COM_EASYTABLEPRO_PREF_USER_MOD_DATE_LABEL_DESC">
		</param>
		<param name="make_tables_sortable" label="COM_EASYTABLEPRO_PREF_MAKE_TABLES_SORTABLE" type="list" default="" description="COM_EASYTABLEPRO_PREF_MAKE_TABLE_SORTABLE_DESC">
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="searchable_by_joomla" type="list" default="" label="COM_EASYTABLEPRO_PREF_SEARCHABLE_IN_JOOMLA" description="COM_EASYTABLEPRO_PREF_SEARCHABLE_IN_JOOMLA_DESC">
			<option value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
	</params>
	<params group="RECORD_VIEW" addpath="/administrator/components/com_easytablepro/elements">
		<param name="title_links_to_table" type="list" default="" label="COM_EASYTABLEPRO_PREF_TITLE_LINKS_TO_TABLE" description="COM_EASYTABLEPRO_PREF_TITLE_LINKS_TO_TABLE_DESC" >
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="title_field" type="easytablefields" default="" label="COM_EASYTABLEPRO_TABLE_TITLE_FIELD" description="COM_EASYTABLEPRO_TABLE_TITLE_FIELD_DESC" />
		<param name="show_next_prev_record_links" type="list" default="" label="COM_EASYTABLEPRO_PREF_NEXT_PREV_LINKS_LABEL" description="COM_EASYTABLEPRO_PREF_SHOW_NEXT_PREV_LINKS">
			<option selected="selected" value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
		<param name="show_linked_table" type="list" default="" label="COM_EASYTABLEPRO_PREF_SHOW_LINKED_TABLE" description="COM_EASYTABLEPRO_PREF_SHOW_LINKED_TABLE_DESC" >
			<option value="">Use Global</option>
			<option value="1">Yes</option>
			<option value="0">No</option>
		</param>
	</params>
	<params group="LINKED_TABLE" addpath="/administrator/components/com_easytablepro/elements">
		<param name="id" type="easytable" default="" label="COM_EASYTABLEPRO_LABEL_SELECT_TABLE" description="COM_EASYTABLEPRO_LABEL_SELECT_TABLE_DESC" />
		<param name="key_field" type="easytablefields" default="" label="COM_EASYTABLEPRO_PREF_SELECT_FIELD" description="COM_EASYTABLEPRO_PREF_LINKED_TABLE_LINK_FIELD_DESC" />
		<param name="linked_key_field" label="COM_EASYTABLEPRO_PREF_LINKED_KEY_FIELD" type="text" default="" description="COM_EASYTABLEPRO_PREF_LINKED_TABLE_LINK_FIELD_ID_DESC" />
	</params>
	<params group="USER_FILTER" addpath="/administrator/components/com_easytablepro/elements">
		<param name="enable_user_filter" type="list" default="" label="COM_EASYTABLEPRO_PREF_ENABLE_USER_FILTER_LABEL" description="COM_EASYTABLEPRO_PREF_ENABLE_USER_FILTER_DESC">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="filter_records_by" type="list" default="" label="COM_EASYTABLEPRO_PREF_FILTER_BY_USER_LABEL" description="COM_EASYTABLEPRO_PREF_FILTER_BY_USER_DESC">
			<option value="name">Name</option>
			<option value="id">ID</option>
		</param>
		<param name="user_filter_field" type="easytablefields" default="" label="COM_EASYTABLEPRO_PREF_SELECT_FIELD" description="COM_EASYTABLEPRO_PREF_USER_FILTER_FIELD_TT" />
	</params>
</root>
